{
  "App": {
    "SelfUrl": "https://localhost:44377",
    "ClientUrl": "http://localhost:4200",
    "CorsOrigins": "https://*.PointsServer.com,http://localhost:4200,https://localhost:44336,https://localhost:44389",
    "RedirectAllowedUrls": "http://localhost:4200,https://localhost:44346,https://localhost:44336,https://localhost:44320"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:8080/"
      }
    }
  },
  "AuthServer": {
    "IssuerUri": "http://localhost:8080"
  },
  "ConnectionStrings": {
    "Default": "mongodb://localhost:27017/PointsServer"
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "localhost",
        "Port": "5672"
      }
    },
    "EventBus": {
      "ClientName": "AuthHandler",
      "ExchangeName": "Schrodinger"
    }
  },
  "Redis": {
    "Configuration": "127.0.0.1"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "DVb2B8QjyeArjCTY"
  },
  "Register": {
    "DomainHeader": "Host"
  },
  "TimeRange": 43200,
  "ExpirationHour": 48,
  "GraphQL": {
    "PortkeyUrl": "https://dapp-aa-portkey-test.portkey.finance/Portkey_V2_DID/PortKeyIndexerCASchema/graphql"
  },
  "Orleans": {
    "ClusterId": "PointsServerSiloCluster",
    "ServiceId": "PointsServerOrleansBasicService",
    "MongoDBClient": "mongodb://127.0.0.1:27017/?maxPoolSize=555",
    "DataBase": "PointsServerOrleansDB"
  },
  "ElasticUris": {
    "Uris": [
      "http://127.0.0.1:9200"
    ]
  },
  "IndexSetting": {
    "NumberOfShards": 5,
    "NumberOfReplicas": 1,
    "IndexPrefix": "PointsServer"
  },
  "Chains": {
    "ChainInfos": {
      "AELF": {
        "BaseUrl": "https://aelf-test-node.aelf.io",
        "IsMainChain": true
      },
      "tDVV": {
        "BaseUrl": "https://tdvv-test-node.aelf.io",
        "IsMainChain": false
      },
      "tDVW": {
        "BaseUrl": "https://tdvw-test-node.aelf.io",
        "IsMainChain": false
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Debug",
        "System": "Warning",
        "Microsoft": "Warning",
        "Orleans": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}{Offset:zzz}][{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/log-{Date}.log",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}{Offset:zzz}][{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}",
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day",
          "retainedFileCountLimit": 15
        }
      }
    ]
  }
}
