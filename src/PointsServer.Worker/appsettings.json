{
  "App": {
    "CorsOrigins": "https://*.CAServer.com,http://localhost:4200,http://*.cn"
  },
  "ConnectionStrings": {
    "Default": "mongodb://localhost:27017/PointsServer?authSource=admin"
  },
  "Redis": {
    "Configuration": "localhost"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "gaMgs0gtsqpYZUTn"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Debug",
        "System": "Debug",
        "Microsoft": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/log-{Date}.log",
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day",
          "retainedFileCountLimit": 5
        }
      }
    ]
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:8068/"
      }
    }
  },
  "ElasticUris": {
    "Uris": [
      "http://localhost:9200"
    ]
  },
  "IndexSetting": {
    "NumberOfShards": 5,
    "NumberOfReplicas": 1,
    "IndexPrefix": "PointsServer"
  },
  "GraphQL": {
    "Configuration": "http://192.168.66.203:8083/AElfIndexer_DApp/PortKeyIndexerCASchema/graphql"
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "localhost",
        "Port": "5672"
      }
    },
    "EventBus": {
      "ClientName": "PointsServer.Worker",
      "ExchangeName": "PointsServer"
    }
  }
}
